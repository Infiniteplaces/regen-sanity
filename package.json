    "@sanity/default-layout": "^2.13.0",
    "@sanity/default-login": "^2.13.0",
    "@sanity/desk-tool": "^2.13.0",
    "@sanity/vision": "^2.13.0",
    "prop-types": "^15.7",
    "react": "^17.0",
    "react-dom": "^17.0",
    "sanity-plugin-intl-input": "^5.2.1"
  },
  "devDependencies": {}
}
